
<% if (number_of_backers > 0) { %>
<div class="project-nav-element group project-nav-element-active"  id="backed">
  Backed
      <div class="qty-for-nav-element">
        <%= number_of_backers %>
      </div>
</div>
<% } %>

<% if (number_of_created > 0) { %>
  <div class="project-nav-element group"  id="created">
    Created
    <div class="qty-for-nav-element">
      <%= number_of_created %>
    </div>
  </div>
  <% } %>

<% if (number_of_comments > 0 || number_of_backers > 0) { %>
<div class="project-nav-element group"  id="comments">
  Comments
    <div class="qty-for-nav-element">
      <%= number_of_comments %>
    </div>
</div>
<% } %>
